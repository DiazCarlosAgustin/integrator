{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-getaway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-getaway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-getaway",
  "projects": {
    "api-getaway": {
      "type": "application",
      "root": "apps/api-getaway",
      "entryFile": "main",
      "sourceRoot": "apps/api-getaway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-getaway/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "parameters": {
      "type": "application",
      "root": "apps/parameters",
      "entryFile": "main",
      "sourceRoot": "apps/parameters/src",
      "compilerOptions": {
        "tsConfigPath": "apps/parameters/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "companies": {
      "type": "application",
      "root": "apps/companies",
      "entryFile": "main",
      "sourceRoot": "apps/companies/src",
      "compilerOptions": {
        "tsConfigPath": "apps/companies/tsconfig.app.json"
      }
    },
    "products": {
      "type": "application",
      "root": "apps/products",
      "entryFile": "main",
      "sourceRoot": "apps/products/src",
      "compilerOptions": {
        "tsConfigPath": "apps/products/tsconfig.app.json"
      }
    },
    "sync": {
      "type": "application",
      "root": "apps/sync",
      "entryFile": "main",
      "sourceRoot": "apps/sync/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sync/tsconfig.app.json"
      }
    },
    "flexxus": {
      "type": "application",
      "root": "apps/flexxus",
      "entryFile": "main",
      "sourceRoot": "apps/flexxus/src",
      "compilerOptions": {
        "tsConfigPath": "apps/flexxus/tsconfig.app.json"
      }
    },
    "services": {
      "type": "application",
      "root": "apps/services",
      "entryFile": "main",
      "sourceRoot": "apps/services/src",
      "compilerOptions": {
        "tsConfigPath": "apps/services/tsconfig.app.json"
      }
    }
  }
}